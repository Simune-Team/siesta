
SystemName             divacancy in graphene
SystemLabel            divacancy-8X8          # Short name for naming files

NumberOfSpecies        2       
NumberOfAtoms          126
%block ChemicalSpeciesLabel
  1  6  C
  2  -6  GC
%endblock ChemicalSpeciesLabel

LatticeConstant       1.435 Ang  
%block LatticeVectors          
 13.8564065    0.00    0.00
  6.9282032   12.00    0.00 
  0.0000000    0.00   20.00
%endblock LatticeVectors

%block k_grid_Monkhorst_Pack
 30    0   0  0.0
 30  -30   0  0.0
  0    0   1  0.0
%endblock k_grid_Monkhorst_Pack
Diag.ParallelOverK    .true. 

      # --- UNFOLD ---
SaveHS			.true.
RefoldingGcutoff	15.0 Ry

%include meshgen.fdf  # Contains %block UnfoldedBandLines. See Src/meshgen.c

%block RefoldingLatticeVectors
  1.732050808   0.00   0.00
  0.866025404   1.50   0.00
  0.00          0.00  20.00
%endblock RefoldingLatticeVectors

XC.functional         GGA           # Exchange-correlation functional
XC.authors            revPBE
PAO.BasisSize         DZP

SpinPolarized       .false.       # Logical parameters are: yes or no
MeshCutoff           250. Ry        # Mesh cutoff. real space mesh 

   # SCF options
MaxSCFIterations      25  #20            # Maximum number of SCF iter
DM.MixingWeight       0.05           # New DM amount for next SCF cycle
DM.Tolerance          0.005 #2.5d-5         # Tolerance in max diff between in and out DM
DM.UseSaveDM          .true.         # to use continuation files
DM.NumberPulay         3

SCFMustConverge      .false.
SolutionMethod        diagon        # OrderN or Diagon

OccupationFunction    MP
OccupationMPOrder     2
ElectronicTemperature  100. meV       # Temp. for Fermi smearing

   # MD options
MD.TypeOfRun           cg            # Type of dynamics:
MD.NumCGsteps          25  #100          # Number of CG steps for coord optimization
MD.MaxCGDispl          0.04 Ang      # Max atomic displacement in one CG step (Bohr)
MD.MaxForceTol         0.05 eV/Ang   # Tolerance in the maximum atomic force (Ry/Bohr)
MD.UseStructFile       .true.        # Uses .STRUCT_IN file
   
# Atomic coordinates
AtomicCoordinatesFormat     ScaledCartesian    
